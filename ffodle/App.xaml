<Application x:Class="ffodle.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ffodle"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/WindowButtons.xaml" />
                <ResourceDictionary Source="Theme/urlBar.xaml" />
                <ResourceDictionary Source="Theme/urlBarButtons.xaml" />
                <ResourceDictionary Source="Theme/ProgressBar.xaml" />
                <ResourceDictionary Source="Theme/Colors.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
